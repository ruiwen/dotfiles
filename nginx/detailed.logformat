
log_format detailed '[$time_local] $host | $http_cf_connecting_ip | $http_x_forwarded_for | $remote_addr | '
                  '"$http_referer" "$http_user_agent" "$http_x_app_agent" '
                  '"$http_cf_ipcountry" "$http_x_requested_with" '
                  '"$request" $status $body_bytes_sent $request_time';

